{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "importHelpers": true,
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "module": "es2020",
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "target": "ES2022",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2017",
      "dom"
    ],
    "paths": {
      "file-input-accessor": [
        "dist/file-input-accessor"
      ],
      "file-input-accessor/*": [
        "dist/file-input-accessor/*"
      ]
    },
    "useDefineForClassFields": false
  }
}